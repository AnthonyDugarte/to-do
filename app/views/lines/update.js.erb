el = document.querySelector("[data-model=\"Line\"][data-id=\"<%= @line.id %>\"]")

if(el) {
    tmp_el = document.createElement("template")
    tmp_el.innerHTML = "<%= j render(@line) %>"

    el.parentElement.replaceChild(tmp_el.content.firstElementChild, el)
}

MicroModal.close("<%= model_modal_id @line %>")