el = document.querySelector("[data-model=\"<%= Project.name %>\"][data-id=\"<%= @project.id %>\"]")

if(el) {
    tmp_el = document.createElement("template")
    tmp_el.innerHTML = "<%= j render(@project, remote: true) %>"

    el.parentElement.replaceChild(tmp_el.content.firstElementChild, el)
}

MicroModal.close("<%= model_modal_id @project %>")